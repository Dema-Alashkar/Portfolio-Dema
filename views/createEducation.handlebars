<section id="create-education">
    <div class="container">
        <div class="row">
            <h1 class="section_title">Create <span class="citron">education</span></h1>
            <div class="create-img-wrapper">
                <img src="" alt="">
            </div>
            <form action="/about/educations/create" method="POST" class="form">

                <div class="from-group col-5">
                    <label class="form-label" for="from-input">Title</label>
                    <input class="form-input" type="text" placeholder="education Name" name="title" required />
                </div>

                <div class="form-group col-5">
                    <label class="form-label" for="form-input">Description</label>
                    <textarea class="form-input" id="input-decsription" placeholder="Type education description" row="5"
                        name="desc"></textarea>
                </div>

                <div class="form-group col-5">
                    <label class="form-label" for="form-input">StartDate</label>
                    <textarea class="form-input" id="input-SatrtDate" placeholder="Type education start date" row="5"
                        name="start"></textarea>
                </div>

                <div class="form-group col-5">
                    <label class="form-label" for="form-input">End Date</label>
                    <textarea class="form-input" id="input-EndDate" placeholder="Type education EndDate" row="5" name="end"></textarea>
                </div>


                <button type="submit" class="btn btn-primary col-1">Submit</button>
            </form>
        </div>
    </div>
</section>

